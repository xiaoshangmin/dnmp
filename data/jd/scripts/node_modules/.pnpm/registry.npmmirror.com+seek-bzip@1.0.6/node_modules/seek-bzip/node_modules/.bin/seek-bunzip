#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/ql/scripts/node_modules/.pnpm/registry.npmmirror.com+seek-bzip@1.0.6/node_modules/seek-bzip/bin/node_modules:/ql/scripts/node_modules/.pnpm/registry.npmmirror.com+seek-bzip@1.0.6/node_modules/seek-bzip/node_modules:/ql/scripts/node_modules/.pnpm/registry.npmmirror.com+seek-bzip@1.0.6/node_modules:/ql/scripts/node_modules/.pnpm/node_modules:/ql/scripts/node_modules:/ql/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/ql/scripts/node_modules/.pnpm/registry.npmmirror.com+seek-bzip@1.0.6/node_modules/seek-bzip/bin/node_modules:/ql/scripts/node_modules/.pnpm/registry.npmmirror.com+seek-bzip@1.0.6/node_modules/seek-bzip/node_modules:/ql/scripts/node_modules/.pnpm/registry.npmmirror.com+seek-bzip@1.0.6/node_modules:/ql/scripts/node_modules/.pnpm/node_modules:/ql/scripts/node_modules:/ql/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/seek-bunzip" "$@"
else
  exec node  "$basedir/../../bin/seek-bunzip" "$@"
fi
