#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/ql/scripts/node_modules/.pnpm/registry.npmmirror.com+he@1.2.0/node_modules/he/bin/node_modules:/ql/scripts/node_modules/.pnpm/registry.npmmirror.com+he@1.2.0/node_modules/he/node_modules:/ql/scripts/node_modules/.pnpm/registry.npmmirror.com+he@1.2.0/node_modules:/ql/scripts/node_modules/.pnpm/node_modules:/ql/scripts/node_modules:/ql/node_modules:/node_modules:/ql/scripts/node_modules/.pnpm/node_modules/he/bin/node_modules:/ql/scripts/node_modules/.pnpm/node_modules/he/node_modules"
else
  export NODE_PATH="$NODE_PATH:/ql/scripts/node_modules/.pnpm/registry.npmmirror.com+he@1.2.0/node_modules/he/bin/node_modules:/ql/scripts/node_modules/.pnpm/registry.npmmirror.com+he@1.2.0/node_modules/he/node_modules:/ql/scripts/node_modules/.pnpm/registry.npmmirror.com+he@1.2.0/node_modules:/ql/scripts/node_modules/.pnpm/node_modules:/ql/scripts/node_modules:/ql/node_modules:/node_modules:/ql/scripts/node_modules/.pnpm/node_modules/he/bin/node_modules:/ql/scripts/node_modules/.pnpm/node_modules/he/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../he/bin/he" "$@"
else
  exec node  "$basedir/../he/bin/he" "$@"
fi
